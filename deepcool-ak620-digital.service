
[Unit]
Description=Displaying the current temperature and utilisation on the Deepcool AK620

[Service]
Type=simple
User=root
ExecStart=/usr/local/deepcool-ak620-digital-linux/env/bin/python3 /usr/local/deepcool-ak620-digital-linux/deepcool-ak620-digital.py
Restart=always

[Install]
WantedBy=default.target
 
